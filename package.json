{
  "name": "what-if-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:client": "dotenv -e .env.development.local -- next dev",
    "dev:server": "dotenv -e .env.development.local -- tsx watch src/server/dev.ts  --tsconfig tsconfig.server.json ",
    "build": "run-s build:client build:server",
    "build:client": "next build",
    "build:server": "dotenv -e .env.production.local -- tsc --project tsconfig.server.json",
    "start": "dotenv -e .env.production.local -- node dist/prod.js",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "db:migrate:dev": "dotenv -e .env.development.local -- prisma migrate dev --skip-seed",
    "db:seed:dev": "dotenv -e .env.development.local -- prisma db seed",
    "db:reset:dev": "dotenv -e .env.development.local -- prisma migrate reset --force --skip-seed",
    "db:studio:dev": "dotenv -e .env.development.local -- prisma studio",
    "db:migrate:prod": "dotenv -e .env.production.local -- prisma migrate deploy",
    "db:seed:prod": "dotenv -e .env.production.local -- prisma db seed",
    "db:reset:prod": "dotenv -e .env.production.local -- prisma migrate reset --force --skip-seed",
    "db:studio:prod": "dotenv -e .env.production.local -- prisma studio",
    "locize:format": "locize format ./public/locales"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.3",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@fontsource/roboto": "5.0.8",
    "@fontsource/roboto-mono": "5.0.16",
    "@hookform/resolvers": "3.3.4",
    "@mui/icons-material": "5.15.11",
    "@mui/lab": "5.0.0-alpha.166",
    "@mui/material": "5.15.11",
    "@mui/x-data-grid": "6.19.5",
    "@mui/x-date-pickers": "6.19.5",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "5.10.2",
    "@tanstack/react-query": "5.24.1",
    "@trpc/client": "11.0.0-next-beta.294",
    "@trpc/next": "11.0.0-next-beta.294",
    "@trpc/react-query": "11.0.0-next-beta.294",
    "@trpc/server": "11.0.0-next-beta.294",
    "argon2": "0.40.1",
    "bullmq": "5.3.3",
    "date-fns": "3.3.1",
    "i18next": "23.10.0",
    "ioredis": "5.3.2",
    "mitt": "3.0.1",
    "next": "14.1.0",
    "next-auth": "4.24.6",
    "next-i18next": "15.2.0",
    "next-multiparty": "0.7.0",
    "notistack": "3.0.1",
    "overlayscrollbars": "2.5.0",
    "overlayscrollbars-react": "0.5.4",
    "pino": "8.19.0",
    "pino-pretty": "10.3.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "7.50.1",
    "react-i18next": "14.0.5",
    "serve-handler": "6.1.5",
    "superjson": "2.2.1",
    "swr": "2.2.5",
    "ulidx": "2.3.0",
    "usehooks-ts": "2.15.1",
    "uuid": "9.0.1",
    "ws": "8.16.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@next/eslint-plugin-next": "14.1.0",
    "@tanstack/react-query-devtools": "5.24.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/serve-handler": "^6.1.4",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "dotenv-cli": "7.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "9.0.11",
    "locize-cli": "^8.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "prisma": "5.10.2",
    "tsx": "4.7.1",
    "typescript": "^5"
  },
  "engines": {
    "node": "^20"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
